        <script>
        	$(document).ready(function(){
                    var classExist = $('body').find('.datatable').length;
                    if(classExist > 0){
                        $(".datatable").dataTable().fnSettings().aoDrawCallback.push({
                            "fn": function () {
                                removeEmptyDropDown();
                            }
                        });
                    }
	        });

	        function removeEmptyDropDown(){
	            $('.selectDropdown').each(function(){
	                var isLiTagFound = $(this).find('li').length;
	                if (isLiTagFound == 0) {
	                    $(this).prev().remove();
	                }
	            });
	        }
        </script>
