{% extends "DhiUserBundle::layout.html.twig" %}
{% block body %}
    {% set brand = app.session.get('brand') %}
                {% set brandName = '' %}
                {% set brandDomain = '' %}
                
                {% if brand %}
                    {% set brandName = brand.name %}
                    {% set brandDomain = brand.domain %}
                    
                {% endif %}
	{% if banners is not empty  %}
		<section class="mainBanner">
				<div class="container-fluid">
						<div class="row">
								<div class="slider single-item" style="display:none;">
									{% for banner in banners %}
                                        <div class="multiple">
							<img src="{{banner.bannerImange}}" alt="{{ banner.bannerAltText }}" class="alignnone size-full wp-image-320" />
                                        </div>
                                    {% endfor %}
<div class="multiple">
        <img src="{{ asset('bundles/dhiuser/images/banner_5.jpg') }}" alt="banner-img-5" class="alignnone size-full wp-image-320" />{% if not app.user %}<a href="{{ url('fos_user_registration_register') }}" class="slider_link3-2"></a>{% endif %}<a href="https://{{brandDomain}}/" class="slider_link3-3" target="_blank"></a><a href="https://play.google.com/store/apps/details?id=com.vianeos.exchangevue" class="slider_link3-4" target="_blank"></a>
        <div class="banner_5_dec">
                {% if not app.user %}
                        <a href="{{ url('fos_user_registration_register') }}">
                                <h4 class="signup_banner_text">Sign Up for an Account to Enjoy Our Basic Package Today!<img src="{{ asset('bundles/dhiuser/images/banner_btn_cir.png') }}"></h4>
                                <h5>We just added new channels - including</h5>
                                <h3>FOX, ABC, CBS, NBC and More!</h3>
                        </a>
                {% else %}
                        <h4 class="signup_banner_text">Sign Up for an Account to Enjoy Our Basic Package Today!<img src="{{ asset('bundles/dhiuser/images/banner_btn_cir.png') }}"></h4>
                        <h5>We just added new channels - including</h5>
                        <h3>FOX, ABC, CBS, NBC and More!</h3>
                {% endif %}
        </div>
</div>
<div class="multiple">
        <img src="{{ asset('bundles/dhiuser/images/banner-1-no-free.jpg') }}" alt="banner-img-3" class="alignnone size-full wp-image-320" />
        {% if not app.user %}
                <a href="{{ url('fos_user_registration_register') }}" class="slider_link"></a>
        {% endif %}
</div>
<div class="multiple">
